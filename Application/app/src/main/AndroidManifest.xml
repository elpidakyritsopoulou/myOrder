<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myorder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_1_"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyOrder"
        tools:replace="android:label, android:icon, android:theme">
        <activity android:name=".PopActivity" />
        <activity android:name=".Bill" />
        <activity android:name=".Menu" />
        <activity android:name=".Staff" />
        <activity android:name=".elegant_button" />
        <activity android:name=".SignIn" />
        <activity android:name=".Register" />
        <activity android:name=".payment_method" />
        <activity android:name=".order_pay" />
        <activity android:name=".Tables" />
        <activity android:name=".Order" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>